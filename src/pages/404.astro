---
import Layout from '../layouts/Layout.astro'
import Card from '../components/Card.astro'
import Hero from '../components/marinecorps/Hero.astro'
import Mission from '../components/marinecorps/Mission.astro'
import Impact from '../components/marinecorps/Impact.astro'
import Countdown from '../components/marinecorps/Countdown.astro'
import Padding from '../components/utils/Padding.astro'
import Products from '../components/marinecorps/Products.astro'

import "../styles/global.css"
---

<Layout title="404">
  <div class="max-h-screen overflow-y-scroll scrollbar-hide snap-start snap-y snap-mandatory">
    <div class="h-screen snap-start md:py-20">
      <Hero
        title="404"
        content="Page not found."
        link="/"
        linkText='GO HOME'
        padding='pt-24'
      />
    </div>
  </div>
</Layout>
